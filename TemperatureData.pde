//Member variables
//A Date object
//minTemperature
//maxTemperature
//snowFall (for bonus marks)

class TemperatureData{
  
  Date date;
  float minTemp;
  float maxTemp;
  float snowFall;
  
  TemperatureData(Date date, float minTemp, float maxTemp, float snowFall){
    this.date = date;
    this.minTemp = minTemp;
    this.maxTemp = maxTemp;
    this.snowFall = snowFall;
    
  }
  
  
}
